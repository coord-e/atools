#!/bin/bash
# -*- coding: utf-8 -*-
# vim:ft=sh :

contest_id=$1

problems=$(get_api_info problems)
[ -z "$contest_id" ] \
  && jq ".[].title + \" (\" + .[].id + \")\"" -r <<< $problems \
  || jq ".[] | if .contest_id == \"${contest_id}\" then .title + \" (\" + .id + \")\" else empty end" -r <<< $problems

